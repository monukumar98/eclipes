package LiveApr;

import java.util.*;

public class Doubtspat {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int[] a = new int[n];
		for (int i = 0; i < n; i++) {
			a[i] = s.nextInt();
		}
		int t = s.nextInt();
		Arrays.sort(a);
		int l = 0, h = n - 1;
		while (l < h) {
			int sum = a[l] + a[h];
			if (sum == t) {
				System.out.println(a[l] + " and " + a[h]);
			}
			if (sum > t)
				h--;
			else
				l++;
		}
	}
}
