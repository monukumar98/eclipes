package LiveApr;

import java.util.*;

public class Doubtspat {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int[] arr = new int[2000];
		int i = 0;
		int n1 = n;
		while (n != 0) {
			arr[i++] = s.nextInt();
			arr[i++] = s.nextInt();
			n--;
		}
		Prime(n1, arr);
	}

	public static void Prime(int n, int[] a) {
		int i = -1, flag, a1, a2;
		while (n != 0) {
			a1 = a[++i];
			a2 = a[i++];
			int m = 0;
			flag = 0;
			for (int j = a1; j <= a2; j++) {
				if (j == 2)
					m++;
				for (int k = 2; k <= j / 2; k++)
					if (j % k == 0) {
						flag = 1;
						break;
					}
				if (flag == 0)
					m++;
			}
			System.out.println(m);
			n--;
		}
	}
}