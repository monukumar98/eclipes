package LiveApr;

import java.util.*;

public class Doubtspat {
	public static void main(String args[]) {
		int n, m;
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		m = sc.nextInt();
		int[][] a = new int[n][m];
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				a[i][j] = sc.nextInt();
			}
		}
		int t = sc.nextInt();
		int r = 0;
		int s = 0, e = n - 1, mid;
		m = m - 1;
		n = n - 1;
		while (s <= e) {
			mid = (s + e) / 2;
			if (a[n][0] == t) {
				System.out.print(1);
				return;
			} else if (a[mid][0] < t) {
				s = mid + 1;
				r = mid;
			} else {
				e = mid - 1;
			}
		}
		s = 0;
		e = n;
		while (s <= e) {
			mid = (s + e) / 2;
			if (a[r][mid] == t) {
				System.out.print(1);
				return;
			} else if (a[r][mid] < t) {
				s = mid + 1;
			} else {
				e = mid - 1;
			}
		}
		System.out.print(0);
	}
}