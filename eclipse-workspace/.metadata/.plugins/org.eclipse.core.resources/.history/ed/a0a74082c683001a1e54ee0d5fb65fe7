package LiveApr;

import java.util.*;
import java.util.Scanner;

public class Doubtspat {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int fn = 0, sn = 0, next = 1, i = 0, j = 0;
		System.out.println(0);
		for (i = 1; i <= n; i++) {
			for (j = 1; j <= i; j++) {
				System.out.print(sn+" ");
				int sum= sn+next;
				sn=next;
				next=sum;

			}
			System.out.println();
		}
		System.out.println();
	}
}
